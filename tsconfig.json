  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2022",
    "moduleResolution": "Node",
    "strict": false,
    "noImplicitAny": true,
    "declaration": false,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "exclude": ["dist", "node_modules"]
}
